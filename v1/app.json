{
  "expo": {
    "name": "NG Tube",
    "slug": "ng-tube",
    "version": "1.0.0",
    "orientation": "portrait",
    "splash": {
      "image": "./public/assets/splash.png",
      "resizeMode": "cover"
    },
    "icon": "./assets/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "svg",
        "ts",
        "tsx"
      ]
    },
    "description": ""
  }
}
